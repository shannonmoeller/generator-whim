{
  "name": "<%= slug %>",
  "version": "1.0.0",
  "description": "<%= description %>",
  "main": "src/index.js",
  "scripts": {
    "coveralls": "nyc report -r text-lcov | coveralls",
    "pretest": "xo src/*.js test/*.js",
    "test": "nyc ava -v",
    "posttest": "nyc report -r lcov",
    "watch": "chokidar '{src,test}/**/{*,.*}' -c 'npm test'"
  },
  "repository": "shannonmoeller/<%= slug %>",
  "keywords": [
    "concat",
    "join",
    "merge",
    "pack",
    "path",
    "sprite",
    "svg",
    "svgs",
    "<%= slug %>",
    "symbol",
    "symbols"
  ],
  "author": "Shannon Moeller <me@shannonmoeller.com> (http://shannonmoeller.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shannonmoeller/<%= slug %>/issues"
  },
  "homepage": "https://github.com/shannonmoeller/<%= slug %>",
  "dependencies": {},
  "devDependencies": {
    "ava": "^0.12.0",
    "chokidar-cli": "^1.2.0",
    "coveralls": "^2.11.6",
    "nyc": "^6.0.0",
    "xo": "^0.13.0"
  },
  "engines": {
    "node": ">= 0.12"
  },
  "xo": {
    "rules": {
      "brace-style": [2, "allman"],
      "eqeqeq": [2, "allow-null"],
      "object-curly-spacing": [2, "always"]
    }
  }
}
